<div class="col s12 center">
	<div class="titulo_inicio">Editar Porcentagem de Comissão de Nível de Usuários</div>
	
	
	<%if(usuario.nivel == 4 || usuario.nivel == 5){ %>
		
		
		<form method="POST" action="" enctype="multipart/form-data">
			
			
			<div class="card light row">
				<input type="hidden" name="id" value="<%= data[0].id %>">
				
				<div class="input-field col s12">
					<label for="aviso">Nivel</label>
					<input disabled type="text" value="<%= data[0].nivel %>">
				</div>
				
				<div class="input-field col s12">
					<label for="porcentagem">Porcentagem</label>
					<input name="porcentagem" type="text" value="<%= data[0].porcentagem %>">
				</div>				
				
				<button data-href="/sistema/administracao/porcentagem-comissao/atualizar" data-action="<%= data.link_sistema%>/administracao/porcentagem-comissao/" class="btn secondary ajax-submit-update margin-b-10">
					Salvar <i class="far fa-save-o" aria-hidden="true"></i>
				</button>
				
				
				
			</div>
		</form>
		
		
		
		
		
		<%}else{%>
			<div class="card light">Você não possui permissão para acessar esse conteúdo</div>
			<%}%>
			
		</div>
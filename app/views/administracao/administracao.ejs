<div class="col s12 center">
	<div class="titulo_inicio">Administração</div>
	
	
	<%if(usuario.nivel == 4 || usuario.nivel == 5){ %>


	<div class="card light row">


		<div class="equal-columns margin-b-10">
			<div class="col s6 margin-t-10">
				<a href="<%= data.link_sistema%>/administracao/noticias" class="ajax-load btn white black-text btn-full-width">
					Noticias
				</a>
			</div>

			<div class="col s6 margin-t-10">
				<a href="<%= data.link_sistema%>/administracao/avisos" class="ajax-load btn white black-text btn-full-width">
					Avisos 
				</a>
			</div>

		</div>

		<div class="equal-columns margin-b-10">

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/usuarios" class="ajax-load btn white black-text btn-full-width">
					Usuário
				</a>
			</div>

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/coaching" class="ajax-load btn white black-text btn-full-width">
					Coaching
				</a>
			</div>

		</div>



		<div class="equal-columns margin-b-10">

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/pedidos-saques" class="ajax-load btn white black-text btn-full-width" style="position: relative;">
					Aprovação Saques
					<%if(data.ver_saque.length > 0){%>
					<div class="red" style="height: 20px;width: 20px; border-radius: 50%; position: absolute;bottom:0;right: 5px; "></div>
					<%}%>
				</a>
			</div>

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/pedidos-aportes" class="ajax-load btn white black-text btn-full-width" style="position: relative;">
					Aprovação Aportes
					
					<%if(data.ver_aporte.length > 0){%>
					<div class="red" style="height: 20px;width: 20px; border-radius: 50%; position: absolute;bottom:0;right: 5px; "></div>
					<%}%>

				</a>
			</div>
		</div>

		<div class="equal-columns margin-b-10">

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/caixa" class="ajax-load btn white black-text btn-full-width">
					Caixa
				</a>
			</div>

			<div class="col s6">
				<a href="<%= data.link_sistema%>/administracao/comissoes" class="ajax-load btn white black-text btn-full-width">
					Comissões
				</a>
			</div>


		</div>


	</div>




	<%}else{%>
	<div class="card light">Você não possui permissão para acessar esse conteúdo</div>
	<%}%>

</div>
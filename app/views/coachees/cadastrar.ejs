
<div class="col s12 center coaching_container">
	<div class="titulo_inicio">Adicionar Novo Coach</div>
	
	<div class="card light">
		
		<form method="POST" action="" enctype="multipart/form-data">	
			<div class="input-field col s12">
				<label for="login">Login</label>
				<input type="text" name="login" required="true">
			</div>
			
			<div class="input-field col s12">
				<label for="nome">Nome</label>
				<input required="true" class="validate" type="text" name="nome">
			</div>

			<div class="input-field col s12">
					<label for="email">Email</label>
					<input required="true" class="validate" type="text" name="email">
				</div>
			
			<div class="input-field col s12">
				<label for="telefone">Telefone</label>
				<input type="text" name="telefone" required="true" class="tel">
			</div>
			
			<div class="input-field col s12">
				<label>Cripto Wallet</label>
				<input type="text">
			</div>
			
			<button data-href="/sistema/coachees/cadastrar/" class="btn waves-effect waves-light green ajax-submit" data-action="/sistema/coachees/">
				Cadastrar
			</button>
			
		</form>		
	</div>	
</div>


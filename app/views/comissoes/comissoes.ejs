
<div class="col s12 center coaching_container">
	<div class="titulo_inicio">Comissões</div>

	<div class="card light">
		<strong>Total em carteira:</strong>R$<%= data.valor_total_coachees[0].total_todos_coachees %>
	</div>


	<div class="card light">
		<table>
			<thead>
				<tr>
					<th>Valor</th>
					<th>Data Cadastro</th>
				</tr>
				
			</thead>
			<tbody>
				<%
				if (typeof data.comissoes != 'undefined' && data.comissoes.length > 0) {
					for (var i = data.comissoes.length - 1; i >= 0; i--) { %>
					<tr>
						<td>R$<%= data.comissoes[i].valor %></td>
						<td><%= data.comissoes[i].data_cadastro %></td>
					</tr>
					
					<%}}else{ %>
					<tr>
						<td class="center" colspan="2">Você não possuí nenhuma Comissão!</td>
					</tr>
					
					<%}%>
				</tbody>

			</table>

		</div>


	</div>


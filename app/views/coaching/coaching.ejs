
<div class="col s12 center coaching_container">
	<div class="titulo_inicio">Coaching</div>
	
	
	<%if(usuario.nivel == 0){ %>
		<div>Nome do Coach: <%=data.nome_coach[0].nome %></div>
		
		<div class="center">Progresso</div>
		<div class="progress">
			<div class="determinate" style="width:<%=(100 * data.total_visto[0].numeroVisto)/data.coaching.length %>%"></div>
		</div>
		
		
		
		<%
		if (typeof data.coaching_vistos != 'undefined' && data.coaching_vistos.length > 0) {
			for (var i = 0; i < data.coaching.length; i++) { 
				for (var j = 0; j < data.coaching_vistos.length; j++){
					%>
					
					<% if(data.coaching[i].id == data.coaching_vistos[j].id){ %>
						<div class="col s6">
							<div class="card primary lighten-2">
								<%= data.coaching[i].titulo %>
							</div>
						</div>
						<%} else if((i+1 > data.coaching_vistos.length) && (j+1 == data.coaching_vistos.length)) { %>
							<div class="col s6">
								<div class="card light darken-1">
									Sessão <%=i+1%>
								</div>
							</div>
							
							
							
							<% }
						}
					}
				}else{%>
					
					<%
					if (typeof data.coaching != 'undefined' && data.coaching.length > 0) {
						for (var i = 0; i < data.coaching.length; i++) {  %>
							
							<div class="col s6">
								<div class="card light darken-1">
									Sessão <%=i+1%>
								</div>
							</div>
							
							<% }
						}
					} %>
					
					
					
					<%}else{%>
						<div class="card light">
							Ainda não finalizado para outros usuarios
						</div>
						
						<%}%>
						
						
						
					</div>
					
					